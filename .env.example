# Build Configuration
# Set to 'app' for full functionality, 'web' for landing page only
VITE_BUILD_TARGET=app

# Deepfake Detection (Premium Feature)
# Enable deepfake detection for video analysis
VITE_ENABLE_DEEPFAKE=true

# Deepfake Provider
# Options: 'truthscan' | 'hive' | 'sensity'
VITE_DEEPFAKE_PROVIDER=truthscan

# Deepfake API Key
# Get your API key from: https://truthscan.com/api
# Note: This is a premium feature and requires a paid API subscription
VITE_DEEPFAKE_API_KEY=your_api_key_here
